# ZBooks for WooCommerce - Local Environment Variables
# Copy this file to .env.local and fill in your Zoho API credentials
# This file is gitignored and will not be committed

# Zoho Books API Credentials
# Get these from https://api-console.zoho.com/
ZOHO_CLIENT_ID=your_client_id_here
ZOHO_CLIENT_SECRET=your_client_secret_here
ZOHO_REFRESH_TOKEN=your_refresh_token_here

# Zoho Organization ID (found in Zoho Books settings)
ZOHO_ORGANIZATION_ID=your_organization_id_here

# Zoho Datacenter Region: us, eu, in, au, jp
ZOHO_DATACENTER=us
